{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "name": "Profile"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "pommesPoints": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "profile"
    }
  }
}
